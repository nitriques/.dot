{
    "editor.fontSize": 14.5,
    "editor.fontFamily": "Ubuntu Mono",
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        4,
        80,
        100,
        120
    ],
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 125,
    "editor.renderControlCharacters": true,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "editor.acceptSuggestionOnEnter": "smart",
    "files.eol": "\n",
    "files.encoding": "utf8",
    "files.autoGuessEncoding": false,
    "workbench.startupEditor": "none",
    "explorer.openEditors.visible": 0,
    "vim.easymotionMarkerFontFamily": "Ubuntu Mono",
    "vim.mouseSelectionGoesIntoVisualMode": false,
    "vim.overrideCopy": false,
    "vim.useCtrlKeys": false,
    "vim.hlsearch": true,
    "vim.statusBarColorControl": true,
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "telemetry.enableTelemetry": false,
    "phpcs.standard": "PSR2",
    "cSpell.userWords": [
        "autoloader",
        "AVIF",
        "backend",
        "checkmark",
        "comparator",
        "Datasource",
        "devkit",
        "DOCROOT",
        "easymotion",
        "ERRMODE",
        "errornum",
        "executables",
        "fulltext",
        "inited",
        "Inno",
        "memoized",
        "Neovim",
        "overridable",
        "PHP's",
        "phpcs",
        "pixelify",
        "prepended",
        "pstm",
        "SQL's",
        "stdlib",
        "sveltejs",
        "tailwindcss",
        "unlogged",
        "unsanitized",
        "validators",
        "varchar",
        "vercel",
        "webp"
    ],
    "cSpell.language": ",fr,en",
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.gitExplorer.enabled": false,
    "gitlens.hovers.enabled": false,
    "gitlens.codeLens.scopes": [],
    "editor.formatOnPaste": true,
    "gitlens.keymap": "alternate",
    "explorer.enableDragAndDrop": false,
    "explorer.autoReveal": false,
    "window.newWindowDimensions": "inherit",
    "telemetry.enableCrashReporter": false,
    "window.openFoldersInNewWindow": "on",
    "window.restoreWindows": "all",
    "vim.cursorStylePerMode": {
        "normal": "underline",
        "insert": "line-thin",
        "replace": "block-outline"
    },
    "vim.easymotion": false,
    "workbench.colorCustomizations": {
        "statusBar.background": "#5f0000",
        "statusBar.noFolderBackground": "#5f0000",
        "statusBar.debuggingBackground": "#5f0000"
    },
    "window.title": "${dirty}${activeEditorLong}${separator}${rootName}${separator}${appName}",
    "workbench.iconTheme": "material-icon-theme",
    "editor.multiCursorMergeOverlapping": false,
    "files.associations": {
        "*.xsl": "xsl"
    },
    "tabnine.experimentalAutoImports": true,
    "svelte.enable-ts-plugin": true,
    "files.exclude": {
        ".svelte-kit": true,
        ".vercel_build_output": true
    },
    "git.enableStatusBarSync": false,
    "git.rebaseWhenSync": true
}
